{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "plugin:jest/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": "latest"
    },
    "plugins": [
        "react", "jest"
    ],
    "rules": {
        "jest/no-disabled-tests": "off"
    },
    "ignorePatterns": ["serviceWorker.js"]
}
